<table class="table">
  <thead>
    <tr>
      <th scope="col">No #</th>
      <th scope="col">Username</th>
      <th scope="col">Email</th>
    </tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
    <tr>
      <th scope="row"><%= user[:id] %></th>
      <td><%= link_to  user.username, user  %></td>
      <td><%=  user.email %></td>
    </tr>
    <% end %>
  </tbody>
</table>

<a href="/users/new" class="btn btn-success d-flex justify-content-center">NEW USER</a>
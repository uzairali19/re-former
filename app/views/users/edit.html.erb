<h1>Edit User</h1>
<%= form_with model: @user do |form| %>
  <%= form.label "Username :" %>
  <%= form.text_field :username, placeholder: "Your user name here" %>
  <%= form.label "Email Address :" %>
  <%= form.text_field :email, placeholder: "example@example.com" %>
  <%= form.label "Password :" %>
  <%= form.password_field :password %>
  <%= form.submit "Submit" %>
<% end %>